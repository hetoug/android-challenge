<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="@color/darkred">
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="@color/darkred" />
            <corners
                android:radius="10dp" />
        </shape>
    </item>
    <item>
        <shape>
            <solid
                android:color="@color/red" />
            <corners
                android:radius="10dp" />
        </shape>
    </item>
</ripple>
